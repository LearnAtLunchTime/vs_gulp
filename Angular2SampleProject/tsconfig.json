{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "classic"
  },
  "scripts": {
    "buildOnSave": true
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules"
  ]
}
